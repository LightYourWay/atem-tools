{
  "name": "atem-tools",
  "version": "1.0.23",
  "description": "A npm library that enables you to utilize dmx controlled lights as tally lights for ATEM switchers from Blackmagic Design.",
  "main": "dist/atem-tools.js",
  "types": "dist/atem-tools.d.ts",
  "prepublish": "tsc",
  "scripts": {
    "test": "tsc && node dist/atem-tools.js",
    "debug": "tsc && node debug dist/atem-tools.js",
    "start": "node dist/atem-tools.js",
    "build": "tsc"
  },
  "_moduleAliases": {
    "@root": ".",
    "@config": "config",
    "@src": "src",
    "@dist": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LightYourWay/atem-tools.git"
  },
  "keywords": [
    "atem",
    "tally",
    "overlay",
    "OBS",
    "open",
    "source",
    "broadcast",
    "live",
    "production",
    "tools"
  ],
  "author": "Lukas Runge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LightYourWay/atem-tools/issues"
  },
  "homepage": "https://github.com/LightYourWay/atem-tools#readme",
  "dependencies": {
    "atem-connection": "^2.1.0",
    "lodash": "^4.17.21",
    "@types/node": "^13.13.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.150"
  },
  "files": [
    "dist/**/*"
  ]
}
